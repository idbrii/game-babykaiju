config_version=5

[application]

config/name="Baby Kaiju"
config/tags=PackedStringArray("2d", "created_by_me", "platformer")
run/main_scene="res://levels/level_master.tscn"
config/features=PackedStringArray("4.3")
config/icon="res://assets/basic/Circle.png"

[autoload]

Broadcast="*res://code/game/broadcast.gd"
DebugDraw2D="*res://addons/debugdraw2d/DebugDraw2D.gd"
SceneSequencer="*res://code/common/scenesequencer.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"
window/size/fullscreen=true

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_sparrow/plugin.cfg", "res://addons/rebuild-anim/plugin.cfg", "res://addons/toggle-external-editor/plugin.cfg")

[global_group]

pushable="Objects that can be pushed by the player."
Spawnpoint=""

[importer_defaults]

texture={
"compress/channel_pack": 0,
"compress/hdr_compression": 1,
"compress/high_quality": false,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
